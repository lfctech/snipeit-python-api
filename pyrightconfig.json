{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "reportMissingImports": "warning",
  "reportMissingModuleSource": "warning",
  "include": [
    "snipeit"
  ],
  "exclude": [
    "tests",
    ".venv",
    "build",
    "dist",
    "**/__pycache__"
  ]
}
